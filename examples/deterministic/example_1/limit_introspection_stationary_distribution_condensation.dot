strict digraph "" {
	graph [d2tgraphstyle="every node/.style={draw}",
		mapping="{5: 0, 10: 1, 11: 1, 14: 1, 15: 1, 1: 2, 2: 2, 3: 2, 4: 2, 6: 2, 7: 2, 8: 2, 9: 2, 12: 2, 13: 2, 0: 3}",
		overlap=false
	];
	node [color=white,
		label="\N",
		shape=circle,
		style=filled
	];
	0	[fillcolor="#BBCCEE",
		label=5,
		members="{5}"];
	1	[fillcolor="#FFCCCC",
		label="10, 11, 14, 15",
		members="{10, 11, 14, 15}"];
	2	[fillcolor="#CCDDAA",
		label="1, 2, 3, 4, 6, 7, 8, 9, 12, 13",
		members="{1, 2, 3, 4, 6, 7, 8, 9, 12, 13}"];
	2 -> 0;
	2 -> 1;
	3	[fillcolor="#EEEEBB",
		label=0,
		members="{0}"];
	3 -> 2;
}
